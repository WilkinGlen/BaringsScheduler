<MudStack Row>
    @{
        var firstIcon = this.GetLastIcon();
        var secondIcon = this.GetSecondLastIcon();
        var thirdIcon = this.GetThirdLastIcon();
    }
    <MudStack Row Spacing="1">
        <MudTooltip Text="@thirdIcon.Message">
            <MudIcon Icon="@thirdIcon.Icon" Color="@thirdIcon.IconColor" />
        </MudTooltip>
        <MudTooltip Text="@secondIcon.Message">
            <MudIcon Icon="@secondIcon.Icon" Color="@secondIcon.IconColor" />
        </MudTooltip>
        <MudTooltip Text="@firstIcon.Message">
            <MudIcon Icon="@firstIcon.Icon" Color="@firstIcon.IconColor" />
        </MudTooltip>
        <MudText Style="margin-top: 0.2rem;" Typo="Typo.subtitle2">@this.LastExecutionDateTime</MudText>
    </MudStack>
</MudStack>
